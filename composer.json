{
  "name": "webdiver/billie-sdk",
  "license": "proprietary",
  "type": "project",
  "description": "SDK PHP for Billie.io API",
  "homepage": "https://www.billie.io/",
  "authors": [
    {
      "name": "Marcel Barten"
    }
  ],
  "require": {
    "php": "^5.6",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "6.3.3",
    "symfony/validator": "3.4.23"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "squizlabs/php_codesniffer": "^3.4",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "autoload": {
    "psr-4": { "Billie\\" : "lib/" }
  },
  "autoload-dev": {
    "psr-4": { "Billie\\Tests\\" : "tests/" }
  }
}
