{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "aws/aws-sdk-php-symfony": "^2.2",
        "billie/monitoring-bundle": "^3.0",
        "billie/pdo-bundle": "^2.1",
        "doctrine/annotations": "^1.10",
        "egulias/email-validator": "^2.1",
        "eightpoints/guzzle-bundle": "^8.1",
        "emag-tech-labs/rabbitmq-bundle": "^2.1",
        "farmatholin/segment-io-bundle": "^1.3",
        "flagception/flagception-bundle": "^3.0",
        "globalcitizen/php-iban": "^2.8",
        "illuminate/support": "^7.13",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-bundle": "^1.5",
        "ozean12/amqp-pack": "^1.1.0",
        "ozean12/amqp-transfers": "^0.3.55",
        "ozean12/graphql-repository-client": "^1.0",
        "ozean12/money": "^3.0",
        "ramsey/uuid": "^3.9",
        "robmorgan/phinx": "^0.12.1",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.7",
        "symfony/framework-bundle": "4.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/stopwatch": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/workflow": "4.4.*",
        "symfony/yaml": "4.4.*",
        "zircote/swagger-php": "^3.0"
    },
    "require-dev": {
        "behat/mink": "^1.8",
        "behat/mink-goutte-driver": "^1.2",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^3.3",
        "donatj/mock-webserver": "^2.1",
        "duxet/json_spec": "^1.2",
        "memio/spec-gen": "^0.9.0",
        "phpspec/phpspec": "^5.1",
        "symfony/phpunit-bridge": "^5.1",
        "fzaninotto/faker": "^1.9"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "spec\\App\\": "bdd/spec/",
            "App\\Tests\\Functional\\Context\\": "bdd/features/bootstrap/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/MonitoringBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/PdoBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/amqp-pack"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/amqp-transfers"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/amqp-transfers-serializer"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/graphql-repository-client"
        },
        {
          "type": "vcs",
          "url": "https://github.com/ozean12/money-php"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
