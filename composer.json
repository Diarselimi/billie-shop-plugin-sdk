{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "aws/aws-sdk-php-symfony": "^2.0",
        "billie/monitoring-bundle": "^2.0",
        "billie/pdo-bundle": "dev-master",
        "doctrine/annotations": "^1.6",
        "donatj/mock-webserver": "^2.0",
        "egulias/email-validator": "^2.1",
        "eightpoints/guzzle-bundle": "^7.3",
        "ocramius/proxy-manager": "2.2.0",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "pusher/pusher-php-server": "^3.0",
        "ramsey/uuid": "^3.7",
        "robmorgan/phinx": "^0.9.2",
        "symfony/console": "*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "*",
        "symfony/monolog-bundle": "*",
        "symfony/proxy-manager-bridge": "*",
        "symfony/security-bundle": "4.2.*",
        "symfony/serializer": "*",
        "symfony/stopwatch": "*",
        "symfony/validator": "*",
        "symfony/workflow": "*",
        "symfony/yaml": "*",
        "vria/nodiacritic": "^0.1.2"
    },
    "require-dev": {
        "behat/mink": "^1.7",
        "behat/mink-goutte-driver": "^1.2",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^3.1",
        "duxet/json_spec": "dev-master#6ce78a41659e621c6c7883d2b8916aa5353eda9d",
        "memio/spec-gen": "^0.9.0",
        "phpspec/phpspec": "^5.0",
        "phpunit/phpunit": "^7.3",
        "symfony/dom-crawler": "*",
        "symfony/dotenv": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/MonitoringBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/PdoBundle"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C94R0H1R4YH60QWA3QWDAG0R",
            "allow-contrib": true,
            "require": "4.2.*"
        }
    }
}
