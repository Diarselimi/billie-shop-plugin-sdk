    "require": {
        "php": "^7.4",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "aws/aws-sdk-php-symfony": "^2.2",
        "billie/monitoring-bundle": "^3.1.1",
        "billie/pdo-bundle": "^2.1",
        "caseyamcl/guzzle_retry_middleware": "^2.6",
        "doctrine/annotations": "^1.10",
        "egulias/email-validator": "^2.1",
        "eightpoints/guzzle-bundle": "^8.1",
        "farmatholin/segment-io-bundle": "^1.3",
        "flagception/flagception-bundle": "^3.0",
        "globalcitizen/php-iban": "3.03",
        "illuminate/support": "^7.13",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-bundle": "^1.5",
        "neitanod/forceutf8": "^2.0",
        "ozean12/amqp-pack": "^2.2",
        "ozean12/amqp-transfers": "^0.4",
        "ozean12/banco-sdk-php": "^1.0",
        "ozean12/borscht-client": "^1.0",
        "ozean12/company-strict-match-bundle": "^1.0",
        "ozean12/graphql-repository-client": "^1.0",
        "ozean12/invoice-butler-client": "^1.1",
        "ozean12/money": "^3.3",
        "ozean12/sepa-client": "^1.3",
        "ozean12/smaug-client": "^1.0.0",
        "ozean12/support": "^3.2",
        "ozean12/watson-client": " ^1.0",
        "ramsey/uuid": "^3.9",
        "robmorgan/phinx": "^0.12.1",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.7",
        "symfony/framework-bundle": "4.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/polyfill-php80": "^1.0",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/stopwatch": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/workflow": "4.4.*",
        "symfony/yaml": "4.4.*",
        "zircote/swagger-php": "3.0.4"
    },
    "require-dev": {
        "behat/mink": "^1.8",
        "behat/mink-goutte-driver": "^1.2",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^3.3",
        "coduo/php-matcher": "^6.0",
        "donatj/mock-webserver": "^2.3",
        "duxet/json_spec": "^1.2",
        "fzaninotto/faker": "^1.9",
        "ozean12/php-cs-fixer": "^1.2",
        "ozean12/pre-commit-hooks": "^2.0",
        "phpspec/phpspec": "^6.2",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "spec\\App\\": "bdd/spec/",
            "App\\Tests\\Functional\\Context\\": "bdd/features/bootstrap/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "repositories": {
        "ozean12-invoice-butler-client": {
            "type": "vcs",
            "url": "https://github.com/ozean12/invoice-butler-client-php"
        },
        "ozean12-borscht-client": {
            "type": "vcs",
            "url": "https://github.com/ozean12/borscht-client-php"
        },
        "ozean12-sepa-client": {
            "type": "vcs",
            "url": "https://github.com/ozean12/sepa-client-php"
        },
        "ozean12-support": {
            "type": "vcs",
            "url": "https://github.com/ozean12/support-php"
        },
        "ozean12-banco-sdk": {
            "type": "vcs",
            "url": "https://github.com/ozean12/banco-sdk-php"
        },
        "ozean12-company-strict-match-bundle": {
            "type": "vcs",
            "url": "https://github.com/ozean12/CompanyStrictMatchBundle"
        },
        "ozean12-monitoring-bundle": {
            "type": "vcs",
            "url": "https://github.com/ozean12/MonitoringBundle"
        },
        "ozean12-pdo-bundle": {
            "type": "vcs",
            "url": "https://github.com/ozean12/PdoBundle"
        },
        "ozean12-amqp-pack": {
            "type": "vcs",
            "url": "https://github.com/ozean12/amqp-pack"
        },
        "ozean12-amqp-transfers": {
            "type": "vcs",
            "url": "https://github.com/ozean12/amqp-transfers"
        },
        "ozean12-amqp-transfers-serializer": {
            "type": "vcs",
            "url": "https://github.com/ozean12/amqp-transfers-serializer"
        },
        "ozean12-graphql-repository-client": {
            "type": "vcs",
            "url": "https://github.com/ozean12/graphql-repository-client"
        },
        "ozean12-money": {
            "type": "vcs",
            "url": "https://github.com/ozean12/money-php"
        },
        "ozean12-php-cs-fixer": {
            "type": "vcs",
            "url": "https://github.com/ozean12/php-cs-fixer"
        },
        "ozean12-pre-commit-hooks": {
            "type": "vcs",
            "url": "https://github.com/ozean12/php-pre-commit-hooks"
        },
        "ozean12-smaug-client": {
            "type": "vcs",
            "url": "https://github.com/ozean12/smaug-client-php"
        },
        "ozean12-watson-client": {
          "type": "vcs",
          "url": "https://github.com/ozean12/watson-client-php"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": ["@auto-scripts", "./vendor/bin/install-pre-commit-hook"],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
