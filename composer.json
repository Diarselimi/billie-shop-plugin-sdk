{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-bcmath": "*",
        "ext-json": "*",
        "aws/aws-sdk-php-symfony": "^2.0",
        "billie/monitoring-bundle": "^2.8",
        "billie/pdo-bundle": "^2.0",
        "doctrine/annotations": "^1.6",
        "egulias/email-validator": "^2.1",
        "eightpoints/guzzle-bundle": "^8.0",
        "farmatholin/segment-io-bundle": "^1.2",
        "globalcitizen/php-iban": "^2.7",
        "illuminate/support": "^5.8",
        "ocramius/proxy-manager": "2.2.0",
        "ozean12/amqp-pack": "^1.0",
        "ozean12/amqp-transfers": "~0.3.17",
        "ozean12/graphql-repository-client": "^1.0",
        "ozean12/money": "^2.1",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "ramsey/uuid": "^3.7",
        "robmorgan/phinx": "^0.11",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.2",
        "symfony/framework-bundle": "4.3.2",
        "symfony/monolog-bundle": "*",
        "symfony/proxy-manager-bridge": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer": "*",
        "symfony/stopwatch": "*",
        "symfony/validator": "*",
        "symfony/workflow": "*",
        "symfony/yaml": "*",
        "zircote/swagger-php": "^3.0"
    },
    "require-dev": {
        "behat/mink": "^1.7",
        "behat/mink-goutte-driver": "^1.2",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^3.1",
        "donatj/mock-webserver": "^2.0",
        "duxet/json_spec": "^1.2",
        "memio/spec-gen": "^0.9.0",
        "phpspec/phpspec": "^5.0",
        "phpunit/phpunit": "^7.3",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "spec\\App\\": "bdd/spec/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/MonitoringBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/PdoBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/amqp-pack"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/amqp-transfers"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/amqp-transfers-serializer"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ozean12/graphql-repository-client"
        },
        {
          "type": "vcs",
          "url": "https://github.com/ozean12/money-php"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C94R0H1R4YH60QWA3QWDAG0R",
            "allow-contrib": true,
            "require": "4.3.*"
        }
    }
}
