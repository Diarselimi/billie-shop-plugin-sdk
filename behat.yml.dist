default:
  suites:
    default:
      contexts:
        - behatch:context:rest
        - json_spec
        - PaellaCoreContext:
            kernel: '@kernel'
  extensions:
    Behatch\Extension: ~
    JsonSpec\Behat\Extension: ~
    Behat\MinkExtension:
      base_url: 'http://paella-core.dev.ozean12.com'
      goutte: ~
    Behat\Symfony2Extension:
      kernel:
        bootstrap: features/bootstrap/bootstrap.php
        class: App\Kernel
